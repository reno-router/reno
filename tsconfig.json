{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "lib": ["esnext", "dom"],
    "module": "amd",
    "moduleResolution": "node",
    "noEmit": true,
    "target": "es2018",
    "baseUrl": ".",
    "paths": {
      "https://*": ["../../.cache/deno/deps/https/*"]
    },
    "plugins": [{
      "name": "deno_ls_plugin"
    }]
  },
  "include": [
    "./**/*.ts"
  ]
}